<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.Shadow?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="647.0" prefWidth="1067.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.vehicleSceneController">
   <children>
      <StackPane prefHeight="647.0" prefWidth="1067.0">
         <children>
            <AnchorPane fx:id="background" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <JFXHamburger fx:id="hamburgerOpen" prefHeight="41.0" prefWidth="55.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
                  <TableView fx:id="tableView" layoutX="82.0" layoutY="155.0" prefHeight="430.0" prefWidth="903.0">
                     <columns>
                        <TableColumn fx:id="id" prefWidth="120.0" text="ID" />
                        <TableColumn fx:id="count" prefWidth="120.0" text="Stolen" />
                        <TableColumn fx:id="modelYear" prefWidth="75.0" text="Model Year" />
                        <TableColumn fx:id="brand" minWidth="0.0" prefWidth="103.0" text="Brand" />
                        <TableColumn fx:id="model" minWidth="0.0" prefWidth="150.0" text="Model" />
                        <TableColumn fx:id="vin" minWidth="0.0" prefWidth="98.0" text="VIN" />
                        <TableColumn fx:id="ownerName" minWidth="0.0" prefWidth="98.0" text="Name of owner" />
                     </columns>
                     <contextMenu>
                        <ContextMenu>
                           <items>
                              <MenuItem mnemonicParsing="false" text="Show details" />
                           </items>
                        </ContextMenu>
                     </contextMenu>
                  </TableView>
                  <JFXButton fx:id="vehicle" layoutX="806.0" layoutY="98.0" mnemonicParsing="false" onAction="#onVehiclesClick" prefHeight="41.0" prefWidth="113.0" style="-fx-background-color: #4C5AA1;" text="Top vehicles" textFill="WHITE">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <effect>
                        <Shadow />
                     </effect>
                  </JFXButton>
                  <JFXButton fx:id="owners" layoutX="920.0" layoutY="98.0" mnemonicParsing="false" onAction="#onOwnersClick" prefHeight="41.0" prefWidth="113.0" style="-fx-background-color: #4C5AA1;" text="Top owners" textFill="WHITE">
